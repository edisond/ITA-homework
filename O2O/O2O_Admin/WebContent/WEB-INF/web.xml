    <filter-name>FoodTypeFilter</filter-name>
    <url-pattern>/main/*</url-pattern>
  </filter-mapping>
  <filter>
    <display-name>AllFoodFilter</display-name>
    <filter-name>AllFoodFilter</filter-name>
    <filter-class>com.oocl.o2o.filter.AllFoodFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>AllFoodFilter</filter-name>
    <url-pattern>/main/newPackage.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>AllFoodFilter</filter-name>
    <url-pattern>/main/UpdatePackageServlet</url-pattern>
  </filter-mapping>
  <servlet>
